{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api/health"
    },
    {
      "source": "/api/v1/health",
      "destination": "/api/v1/health"
    },
    {
      "source": "/api/v1/auth/signup",
      "destination": "/api/v1/auth/signup"
    },
    {
      "source": "/api/v1/auth/login",
      "destination": "/api/v1/auth/login"
    }
  ]
}
